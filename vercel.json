{
  "version": 2,
  "builds": [
    {
      "src": "database/server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/database/server.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/login/login.html",
      "permanent": false
    }
  ]
}